{
    "name": "usap/sentinelisk",
    "description": "An implementation of the Sentry User Manager for Laravel.",
    "keywords": ["laravel", "sentry", "auth", "sentinel"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Iskandar Ali",
            "email": "miskandarmali@gmail.com",
            "homepage": "http://iskandarali.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*@dev",
        "illuminate/contracts": "5.1.*@dev",
        "cartalyst/sentry": "2.1.*",
        "vinkla/hashids": "~2.0",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.1.x@dev",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
            "src/seeds",
            "src/controllers",
            "tests"
        ],
        "psr-4": {
            "Sentinel\\": "src/Sentinel"
        }
    },
    "minimum-stability": "dev"
}
